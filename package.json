{
  "name": "typescript-starter-frontend",
  "version": "0.0.5",
  "description": "Typescript starter for frontend",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --watch",
    "lint": "eslint 'src/**/*.ts'",
    "lint:staged": "npm run build && pretty-quick --staged && lint-staged",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "jest --verbose src",
    "test:watch": "jest --watch",
    "coverage": "jest --collect-coverage --verbose src",
    "release": "standard-version",
    "stats": "webpack --profile --json > stats.json",
    "docs": "typedoc src --ignoreCompilerErrors"
  },
  "author": "Eric Lara <ericzon@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/jest": "^26.0.8",
    "@types/webpack": "4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "standard-version": "^8.0.2",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.4.0",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint:staged",
      "pre-push": "npm t"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix"
  },
  "keywords": [
    "typescript",
    "starter",
    "webpack"
  ]
}
